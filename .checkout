#!/bin/bash
git fetch
if [ $# -eq 0 ]; then
    git checkout dev
    git pull origin dev
else
    if $(git branch | grep -q $1); then
        git checkout $1
        git pull origin $1
    else
        echo That branch does not exist
    fi
fi

composer install
npm install
npm run dev
php artisan migrate:fresh --seed
php artisan serve
