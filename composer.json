{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "fzaninotto/faker": "~1.4",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "martinbean/laravel-sluggable-trait": "^0.3.0",
        "socialiteproviders/generators": "^4.1",
        "socialiteproviders/manager": "^3.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "nunomaduro/collision": "~1.1",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^6.5.6",
        "symfony/thanks": "~1.0",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit-selenium": "^4.1",
        "se/selenium-server-standalone": "^3.9"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "SocialiteProviders\\Avans\\": "SocialiteProviders/Avans/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
